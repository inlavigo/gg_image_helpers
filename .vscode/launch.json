  "version": "0.2.0",

  "configurations": [
    {
      "name": "Run GgImageTools",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/bin/gg_image_tools.dart",
      "args": [
        "mvcd",
        "-i",
        "${workspaceFolder}/test/",
        "-o",
        "/tmp/gg-image-tools-test"
      ],
      "console": "debugConsole"
    },
    {
      "name": "Test Current File",
      "type": "dart",
      "request": "launch",
      "program": "${file}"
    }
  ]
}
